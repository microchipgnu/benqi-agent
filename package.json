{
  "name": "benqi-avax-agent",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev --turbopack\" \"make-agent dev\"",
    "dev-testnet": "concurrently \"next dev --turbopack\" \"make-agent dev -p 3000 -t\"",
    "build": "next build && make-agent deploy -u ${VERCEL_URL:-https://benqi-agent.vercel.app}",
    "build:deploy": "next build && make-agent deploy -u ${VERCEL_URL:-https://benqi-agent.vercel.app}",
    "bitte:deploy": "make-agent deploy -u ${VERCEL_URL:-https://benqi-agent.vercel.app}",
    "start": "next start",
    "lint": "next lint && prettier --check '{src/app,tests}/**/*.{js,jsx,ts,tsx}'",
    "fmt": "next lint --fix && prettier --write '{src/app,tests}/**/*.{js,jsx,ts,tsx}'",
    "test": "jest --config jest.config.ts"
  },
  "dependencies": {
    "@bitte-ai/agent-sdk": "^0.1.9",
    "@cowprotocol/cow-sdk": "^5.10.1",
    "json-stringify-deterministic": "^1.0.12",
    "near-safe": "^0.9.12",
    "next": "^15.2.4",
    "redoc": "^2.4.0",
    "vercel-url": "^0.2.6",
    "viem": "^2.23.15",
    "zerion-sdk": "^0.1.1"
  },
  "devDependencies": {
    "@types/bun": "^1.2.6",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.13",
    "@types/react": "^19.0.12",
    "concurrently": "^9.1.2",
    "dotenv": "^16.4.7",
    "eslint-config-next": "^15.2.4",
    "jest": "^29.7.0",
    "make-agent": "0.2.11",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
}
